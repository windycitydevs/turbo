{
  "name": "@windycitydevs/backfill",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public",
    "typesVersions": {
      "*": {
        "*": [
          "src/*",
          "src/*/index"
        ],
        "packge.json": [
          "./package.json"
        ],
        "bin": [
          "dist/bin.d.ts"
        ]
      }
    }
  },
  "keywords": [
    "cli",
    "algolia",
    "sanity",
    "node",
    "dotenv",
    "tsup"
  ],
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./dist/bin.js",
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ],
      "packge.json": [
        "./package.json"
      ],
      "bin": [
        "dist/bin.d.ts"
      ]
    }
  },
  "sideEffects": true,
  "files": [
    "dist/**/*.{js,cjs,d.ts,d.cts,d.mts,mjs,mts,cts,css}"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./package.json": "./package.json",
    "./bin": "./dist/bin.js"
  },
  "scripts": {
    "build": "yarn release && yarn types",
    "dev": "ncc build ./src/index.ts -o ./dist/",
    "release": "ncc build ./src/index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "prerelease": "rimraf ./dist/",
    "types": "tsc --declaration --esModuleInterop --declarationDir dist",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\" --fix",
    "clean": "rimraf .turbo node_modules dist",
    "publish": "npm publish",
    "package:test": "tsx src/bin.ts --groq Article"
  },
  "devDependencies": {
    "@types/node": "^20.1.4",
    "@types/react": "^18.2.6",
    "@types/yargs": "^17.0.24",
    "@vercel/ncc": "^0.36.1",
    "algoliasearch": "^4.17.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "dotenv-expand": "^10.0.0",
    "eslint": "^8.40.0",
    "eslint-config-custom": "*",
    "next": "^13.4.2",
    "react": "^18.2.0",
    "rimraf": "^5.0.0",
    "tslib": "^2.5.0",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "@sanity/client": "^6.0.1",
    "algoliasearch": "^4.17.0",
    "next": "^13.4.2"
  },
  "dependencies": {
    "@sanity/client": "^6.0.1",
    "algoliasearch": "^4.17.0",
    "axios": "^1.4.0",
    "chalk": "^5.2.0",
    "groq": "^3.10.3",
    "yargs": "^17.7.2"
  }
}
