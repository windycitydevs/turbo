{
  "name": "@windycitydevs/sanity-indexer",
  "version": "1.0.0",
  "files": [
    "dist/**/*.{js,cjs,d.ts,d.mts,mts,mjs,cts,d.cts,css}"
  ],
  "keywords": [
    "algolia",
    "sanity",
    "indexer",
    "next.js",
    "node",
    "dotenv",
    "swcrc"
  ],
  "license": "MIT",
  "type": "module",
  "sideEffects": true,
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "publishConfig": {
    "access": "public",
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ]
      }
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./types": "./dist/types/*.js"
  },
  "scripts": {
    "lint": "TIMING=1 eslint . --ext .js,.jsx,.ts,.tsx",
    "dev": "yarn build",
    "prebuild": "rm -rf dist",
    "build": "yarn build:swc && yarn types",
    "build:swc": "swc src -d dist --copy-files",
    "clean": "rm -rf ./dist",
    "postclean": "yarn types",
    "publish": "npm publish",
    "types": "tsc --emitDeclarationOnly",
    "posttypes": "cd dist && rm -rf tsconfig.tsbuildinfo"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.58",
    "@types/node": "^20.1.5",
    "@types/react": "^18.2.6",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "dotenv-expand": "^10.0.0",
    "eslint": "^8.40.0",
    "eslint-config-custom": "*",
    "next": "^13.4.2",
    "react": "^18.2.0",
    "rimraf": "^5.0.0",
    "sanity": "^3.11.0",
    "tslib": "^2.5.0",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "@sanity/client": "^6.1.0",
    "algoliasearch": "^4.17.0",
    "next": "^13.4.2",
    "react": "^18.2.0",
    "sanity": "^3.11.0"
  },
  "peerDependenciesMeta": {
    "sanity": {
      "optional": true
    },
    "@sanity/client": {
      "optional": true
    }
  },
  "dependencies": {
    "@sanity/client": "^6.1.0",
    "@swc/helpers": "^0.5.1",
    "algoliasearch": "^4.17.0"
  }
}