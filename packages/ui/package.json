{
  "name": "@windycitydevs/ui",
  "version": "1.0.0",
  "files": [
    "dist/**/*.{js,cjs,d.ts,d.cts,css}"
  ],
  "license": "MIT",
  "sideEffects": true,
  "type": "module",
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ],
      "tailwind": [
        "dist/tailwind.d.cts"
      ]
    }
  },
  "publishConfig": {
    "access": "public",
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ],
        "tailwind": [
          "dist/tailwind.d.cts"
        ]
      }
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./tailwind": {
      "require": "./dist/tailwind.cjs"
    },
    "./globals.css": "./dist/globals.css"
  },
  "scripts": {
    "lint": "TIMING=1 eslint . --ext .js,.jsx,.ts,.tsx",
    "dev": "yarn build",
    "prebuild": "rm -rf dist",
    "build": "yarn build:swc && yarn types",
    "build:swc": "swc src -d dist --copy-files",
    "clean": "rm -rf ./dist",
    "postclean": "yarn types",
    "publish": "npm publish",
    "types": "tsc --emitDeclarationOnly && cd dist && rm -rf tsconfig.tsbuildinfo"
  },
  "peerDependencies": {
    "next": "^13.4.5-canary.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.1",
    "clsx": "^1.2.1",
    "sugar-high": "~0.4.6"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.62",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "chokidar": "^3.5.3",
    "eslint": "^8.42.0",
    "eslint-config-custom": "*",
    "eslint-config-next": "^13.4.5-canary.4",
    "next": "^13.4.5-canary.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3"
  }
}