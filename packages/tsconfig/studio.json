{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Sanity",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "downlevelIteration": true,
    "incremental": true,
    "noFallthroughCasesInSwitch": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "target": "ES2015",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "module": "CommonJS"
  }
}
