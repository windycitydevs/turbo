{
  "name": "@windycitydevs/express",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist/ .turbo && tsx vercel-generator.ts",
    "build": "tsup api/index.ts api/server.ts api/logger.ts --format esm --dts && cp -r public/* ./dist",
    "clean": "rimraf dist/ .turbo node_modules",
    "predev": "rimraf dist/ .turbo",
    "dev": "tsup api/index.ts api/server.ts api/logger.ts --format esm --dts --watch --onSuccess \"cp -r public/* ./dist && node dist/index.mjs\"",
    "lint": "tsc --noEmit && TIMING=1 eslint \"src/**/*.ts*\"",
    "start": "node dist/index.mjs",
    "test": "jest --detectOpenHandles"
  },
  "private": true,
  "jest": {
    "preset": "@windycitydevs/jest-presets/jest/node"
  },
  "dependencies": {
    "@vercel/edge": "^0.3.4",
    "@vercel/edge-config": "^0.1.10",
    "@vercel/fs-detectors": "^3.9.2",
    "@vercel/node": "^2.14.2",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@edge-runtime/cookies": "^3.1.0",
    "@edge-runtime/ponyfill": "^2.2.0",
    "@edge-runtime/types": "^2.0.8",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.1.4",
    "@types/supertest": "^2.0.12",
    "@windycitydevs/jest-presets":"*",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "dotenv-expand": "^10.0.0",
    "eslint": "^8.40.0",
    "eslint-config-custom-server": "*",
    "fast-xml-parser": "^4.2.2",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "rimraf": "^5.0.0",
    "supertest": "^6.3.3",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "urlpattern-polyfill": "^8.0.2"
  }
}
