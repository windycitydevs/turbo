{
  "name": "@windycitydevs/express",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist/ .turbo",
    "build": "tsup api/index.ts api/server.ts --format esm --dts && cp -r public/* ./dist",
    "clean": "rimraf dist/ .turbo node_modules",
    "predev": "rimraf dist/ .turbo",
    "dev": "tsup api/index.ts api/server.ts  --format esm --dts --watch --onSuccess \"cp -r public/* ./dist && node dist/index.mjs\"",
    "lint": "tsc --noEmit && TIMING=1 eslint \"src/**/*.ts*\"",
    "start": "node dist/index.mjs",
    "test": "jest --detectOpenHandles"
  },
  "jest": {
    "preset": "@windycitydevs/jest-presets/jest/node"
  },
  "dependencies": {
    "@cloudinary/url-gen": "^1.9.0",
    "@edge-runtime/cookies": "^3.0.4",
    "@edge-runtime/ponyfill": "^2.0.0",
    "@edge-runtime/types": "^2.0.5",
    "@vercel/edge": "^0.2.7",
    "@vercel/edge-config": "^0.1.1",
    "@vercel/fs-detectors": "3.7.10",
    "@vercel/node": "^2.9.2",
    "body-parser": "^1.20.1",
    "cloudinary": "^1.33.0",
    "cookie-parser": "^1.4.6",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.13.0",
    "@types/supertest": "^2.0.12",
    "@windycitydevs/jest-presets":"*",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "dotenv-expand": "^10.0.0",
    "eslint": "^8.34.0",
    "eslint-config-custom-server": "*",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
    "prettier-plugin-organize-imports": "^3.2.2",
    "rimraf": "^4.1.2",
    "supertest": "^6.3.3",
    "tsup": "^6.6.0",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "urlpattern-polyfill": "^6.0.2"
  }
}
